
.look {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	position: fixed;
	z-index: 6666;
	background: #fff;
	width: 256px;
	left: calc(50% - 128px);
	top: calc(50% - 173px);
}
.look-img {
	width: 100%;
}

